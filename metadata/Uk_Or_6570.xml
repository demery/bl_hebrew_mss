               <physDesc rend="visible">
                  
                  
                  
                  <p>Parchment codex of the 14th century. </p>
                  
                  
                  
                  <objectDesc form="codex">
                     
                     
                     
                     <supportDesc material="perg">
                        
                        
                        
                        <support>Parchment.</support>
                        
                        
                        
                        <extent>338 folios<dimensions unit="mm" type="leaf">
                              
                              
                              
                              <height>300</height>
                              
                              
                              
                              <width>220</width>
                           
                           
                           
                           </dimensions>
                           
                           
                           
                           <dimensions type="written" unit="mm">
                              
                              
                              
                              <height>215</height>
                              
                              
                              
                              <width>130</width>
                           
                           
                           
                           </dimensions>
                        
                        
                        
                        </extent>
                        
                        
                        
                        <foliation>
                           
                           
                           
                           <p>338 folios (+ 2 unfoliated paper flyleaves at the beginning and at the end). Two leaves cancelled at the end. </p>
                        
                        
                        
                        </foliation>
                        
                        
                        
                        <collation>
                           
                           
                           
                           <p>Quires of 8 leaves each, occasionally marked by catchwords. </p>
                        
                        
                        
                        </collation>
                     
                     
                     
                     </supportDesc>
                     
                     
                     
                     <layoutDesc>
                        
                        
                        
                        <layout columns="2" ruledLines="30" writtenLines="30">
                           
                           
                           
                           <p>Uniform layout. </p>
                           
                           
                           
                           <p>Pricking (occasionally) and complete hard-point ruling visible. </p>
                           
                           
                           
                           <p>Hair side and flesh side of folios distinguishable. </p>
                           
                           
                           
                           <p>Beginning of a weekly section marked by the word סדר in the margin. The sign פפ stands between the sections. </p>
                           
                           
                           
                           <p>Masorah magna is written in two lines in the upper margin and in three lines in the lower margin. </p>
                        
                        
                        
                        </layout>
                     
                     
                     
                     </layoutDesc>
                  
                  
                  
                  </objectDesc>
                  
                  
                  
