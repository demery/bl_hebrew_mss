                     <title xml:lang="he">סדור מנהג קראים</title>
                     
                     
                     
                     <note>
                        
                        
                        
                        <p>Incomplete. </p>
                     
                     
                     
                     </note>
                  
                  
                  
                  </msItem>
               
               
               
               </msContents>
               
               
               
