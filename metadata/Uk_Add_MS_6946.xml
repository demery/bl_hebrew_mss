               <head>Seder ha-῾olam be-Tana"kh</head>
                    
               
               
               
               <msContents class="lccn.loc.gov/sh85013593" rend="visible">
                        
                  
                  
                  
                  <summary>Biblical genealogy. </summary>
                        
                  
                  
                  
                  <textLang mainLang="la" otherLangs="he ara"/>
                        
                  
                  
                  
                  <msItem rend="visible">
                            
                     
                     
                     
