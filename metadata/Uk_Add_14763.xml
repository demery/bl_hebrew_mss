							<title xml:lang="he-Latn-x-lc">Ruaḥ ḥen</title>
							<title xml:lang="he">רוח חן</title>
						</msItem>
						<msItem rend="">
							<locus from="182v" to="198r"/>
							<title xml:lang="he-Latn-x-lc">Otot ha-shamayim</title>
							<title xml:lang="he">אותות השמים</title>
							<author>
								<persName key="http://viaf.org/viaf/7524651" xml:lang="en">Aristotle</persName>
								<persName key="http://viaf.org/viaf/7524651" xml:lang="he">אריסטו</persName>
							</author>
							<decoNote type="diagram">Diagram of the four main winds illustrating Aristotle's Meterology.<locus n="190v"/>
							</decoNote>
						</msItem>
