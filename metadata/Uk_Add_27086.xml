					<msContents class="lccn.loc.gov/n79090052" rend="visible">
						<summary>A daily prayer book according to the Ashkenazi rite, with a number of poetical pieces.</summary>
						<textLang mainLang="he"/>
						<msItem n="0" rend="visible" class="lccn.loc.gov/n79090052">
							<locus from="1r" to="264v"/>
							<title xml:lang="he-Latn-x-lc">Sidur</title>
							<title xml:lang="he">סדור</title>
						</msItem>
						<msItem rend="">
							<locus from="1v" to="33v"/>
							<title xml:lang="en">Services for week-days</title>
						</msItem>
						<msItem rend="">
							<locus from="33v" to="55v"/>
							<title xml:lang="en">Services for Shabat</title>
						</msItem>
						<msItem rend="">
							<locus from="55v" to="62v"/>
							<title xml:lang="en">Special portions for Rosh ḥodesh, Purim, etc.</title>
						</msItem>
						<msItem rend="">
							<locus from="62v" to="65v"/>
							<title xml:lang="en">Prayers for yamim ṭovim</title>
						</msItem>
						<msItem rend="">
							<locus from="65v" to="86r"/>
							<title xml:lang="en">Services for Pesaḥ</title>
						</msItem>
						<msItem rend="">
							<locus from="86r" to="113r"/>
							<title xml:lang="he-Latn-x-lc">Pirḳe avot</title>
						</msItem>
						<msItem rend="">
							<locus from="113r" to="144r"/>
							<title xml:lang="en">A series of ma῾arivim</title>
							<note>
								<p>For Pesaḥ, folio 113r; for Shavu῾ot, folio 129v; for Sukot, folio 135v.</p>
							</note>
						</msItem>
						<msItem rend="">
							<locus from="144r" to="171v"/>
							<title xml:lang="en">Prayers for Rosh ha-Shanah and Yom Kipur</title>
						</msItem>
						<msItem rend="">
							<locus from="171v" to="186v"/>
