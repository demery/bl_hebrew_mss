						<msItem rend="">
							<locus from="1r" to="1r"/>
							<title xml:lang="he-Latn-x-lc">Tefilah le-shalom Leonora di Asti </title>
							<title xml:lang="he">תפלה לשלום ליאונורה די אסטי </title>
							<note>
								<p>Dated <date calendar="Hebrew">[5]341</date> (<date calendar="Gregorian">1581</date>), <name type="place">Ferara</name>.</p>
							</note>
						</msItem>
						<msItem rend="">
							<locus from="2r" to="2v"/>
							<title xml:lang="he-Latn-x-lc">Sod sefirat ha-῾omer</title>
